{
  "name": "vincent-hackathon-trading-bot",
  "private": false,
  "version": "1.0.0",
  "description": "Vincent AI Agent Hackathon - Multi-Agent Trading System with User-Controlled Permissions",
  "homepage": "https://github.com/ZubeidHendricks/vincent-hackathon-trading-bot",
  "engines": {
    "node": "^20.11.1",
    "pnpm": ">=10.7.0"
  },
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "lint": "pnpm -r lint",
    "clean": "pnpm exec rm -Rf node_modules dist && pnpm --parallel -r run clean",
    "test": "pnpm -r run test",
    "dev": "pnpm build && pnpm -r run dev",
    "prepare": "husky",
    "start": "pnpm -r run start",
    "vincent-hackathon": "pnpm --filter dca-backend vincent-hackathon",
    "vincent-hackathon:dev": "pnpm --filter dca-backend vincent-hackathon:dev",
    "vincent-hackathon:test": "pnpm --filter dca-backend vincent-hackathon:test",
    "link-local-vincent-sdk-dev": "pnpm dotenvx run -- ./linkVincentSdkLocaldev.sh"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "keywords": [
    "vincent",
    "ai-agent",
    "hackathon",
    "trading",
    "cryptocurrency",
    "autonomous",
    "multi-agent",
    "pkp",
    "lit-protocol",
    "user-controlled",
    "policy-governance"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ZubeidHendricks/vincent-hackathon-trading-bot.git"
  },
  "bugs": {
    "url": "https://github.com/ZubeidHendricks/vincent-hackathon-trading-bot/issues"
  },
  "license": "MIT",
  "author": "Vincent AI Agent Hackathon Team",
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.26.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^20.11.30",
    "@types/verror": "^1.10.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-plus": "^1.4.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.29.0",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@lit-protocol/vincent-sdk": "0.0.5",
    "commander": "^14.0.0",
    "consola": "^3.2.3",
    "tslib": "^2.8.1"
  }
}
